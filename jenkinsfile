node {
  stage("Clone the project") {
    git branch: 'master', url: 'https://github.com/wajdi-HUB/testDevopsRepo.git'
  }


    stage('MVN-CLEAN') {
                steps {
                    sh "mvn clean install"
                }
           }

            stage('MVN-COMPILE') {
                 steps {
                    sh "mvn compile"
                 }
            }

            stage('Mvn-Build') {
                steps {
                    script{
                        sh "mvn -Dmaven.test.failure.ignore=true clean package"
                    }
                }
            }

            stage('Mvn-Test') {
               steps {
                   script{
                           sh "mvn test"
                                                 }

                                                   }
                                                   }
}
